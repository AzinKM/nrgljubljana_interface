# This file allows other CMake Projects to find us
# We provide general project information
# and reestablish the exported CMake Targets

# Multiple inclusion guard
if(NOT NRGLJUBLJANA_INTERFACE_FOUND)
set(NRGLJUBLJANA_INTERFACE_FOUND True)

# version
set(NRGLJUBLJANA_INTERFACE_VERSION @PROJECT_VERSION@)
set(NRGLJUBLJANA_INTERFACE_GIT_HASH @PROJECT_GIT_HASH@)

# Root of the installation
set(NRGLJUBLJANA_INTERFACE_ROOT  @CMAKE_INSTALL_PREFIX@)

# Include the exported targets of this project
include(@CMAKE_INSTALL_PREFIX@/lib/cmake/nrgljubljana_interface/nrgljubljana_interface-targets.cmake)

message(STATUS "Found nrgljubljana_interface-config.cmake with version @PROJECT_VERSION@, hash = @PROJECT_GIT_HASH@")

# Was the Project built with Documentation?
set(NRGLJUBLJANA_INTERFACE_WITH_DOCUMENTATION @Build_Documentation@)

endif()
